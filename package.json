{
  "name": "glimmr",
  "productName": "Glimmr",
  "target": "node",
  "version": "0.2.2",
  "description": "Programmatically adjust the brightness of your monitors.",
  "main": ".webpack/main/main.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development electron-forge start",
    "format": "prettier ./src/**/*.{ts,tsx} --write",
    "package": "cross-env NODE_ENV=production electron-forge package",
    "make": "cross-env NODE_ENV=production electron-forge make",
    "version:prev": "ts-node-esm pipeline/version.ts -- --decrement",
    "version:next": "ts-node-esm pipeline/version.ts -- --increment",
    "drop": "ts-node-esm pipeline/drop.ts",
    "draft": "ts-node-esm pipeline/draft.ts",
    "prepublish": "npm run format && npm run version:next && npm run make",
    "publish": "npm run draft && npm run drop",
    "publish:auto": "npm run prepublish && npm run draft -- -- --auto && npm run drop"
  },
  "keywords": [],
  "author": "Glimmr",
  "devDependencies": {
    "@electron-forge/cli": "^6.1.1",
    "@electron-forge/maker-deb": "^6.1.1",
    "@electron-forge/maker-rpm": "^6.1.1",
    "@electron-forge/maker-squirrel": "^6.1.1",
    "@electron-forge/maker-zip": "^6.1.1",
    "@electron-forge/plugin-webpack": "^6.1.1",
    "@herberttn/bytenode-webpack-plugin": "^2.3.1",
    "@types/auto-launch": "^5.0.2",
    "@types/color": "^3.0.3",
    "@types/lodash": "^4.14.194",
    "@types/node-fetch": "^2.6.4",
    "@types/nodemailer": "^6.4.8",
    "@types/prettier": "^2.7.3",
    "@types/react-beautiful-dnd": "^13.1.4",
    "@types/react-dom": "^18.2.4",
    "@vercel/webpack-asset-relocator-loader": "^1.7.3",
    "cli-progress": "^3.12.0",
    "cli-spinners": "^2.9.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.3",
    "electron": "24.3.0",
    "file-loader": "^6.2.0",
    "fork-ts-checker-webpack-plugin": "^7.3.0",
    "gh-release-assets": "^2.0.1",
    "log-symbols": "4.1.0",
    "node-emoji": "^2.1.0",
    "node-loader": "^2.0.0",
    "node-ssh": "^13.1.0",
    "ora": "5.4.1",
    "prettier": "^2.8.8",
    "prompt-confirm": "^2.0.4",
    "raw-loader": "^4.0.2",
