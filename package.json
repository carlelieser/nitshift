    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.11.16",
    "@mui/lab": "^5.0.0-alpha.134",
    "@mui/material": "^5.13.4",
    "@mui/x-date-pickers": "^6.8.0",
    "@reduxjs/toolkit": "^1.9.5",
    "async": "^3.2.4",
    "auto-launch": "^5.0.6",
    "color": "^4.2.3",
