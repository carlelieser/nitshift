    "svg-url-loader": "^8.0.0",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "typescript": "~4.5.4",
    "url-loader": "^4.1.1",
    "worker-loader": "^3.0.8"
  },
